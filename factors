#!/usr/bin/python3
import sys
import math

arg = sys.argv
f = open(arg[-1], 'r')
for line in f:
    flag = 0
    number = int(line)
    for i in range(2, 20):
        if (number % i == 0):
            print("{}={}*{}".format(number, number//i, i))
            flag = 1
            break
    if flag == 0:
        i = int(math.sqrt(number))
        for j in range(i, i+20):
            if number % j == 0:
                print("{}={}*{}".format(number, number//j, j))
                flag = 1
                break
f.close()
